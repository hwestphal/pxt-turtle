{
    "id": "turtle",
    "name": "Turtle",
    "corepkg": "core",
    "bundleddirs": [
        "libs/core"
    ],
    "runtime": {
        "loopsBlocks": true,
        "logicBlocks": true,
        "variablesBlocks": true,
        "mathBlocks": true,
        "functionBlocks": true,
        "listsBlocks": true,
        "textBlocks": true,
        "onStartColor": "#f48942",
        "onStartUnDeletable": true,
        "palette": [
            "#ffffff",
            "#000000",
            "#ff0000",
            "#ff8000",
            "#ffff00",
            "#ff9da5",
            "#00ff00",
            "#b09eff",
            "#00ffff",
            "#007fff",
            "#65471f",
            "#0000ff",
            "#7f00ff",
            "#ff0080",
            "#ff00ff",
            "#999999"
        ]
    },
    "simulator": {
        "autoRun": true,
        "stopOnChange": true,
        "hideFullscreen": true,
        "aspectRatio": 1
    },
    "appTheme": {
        "availableLocales": [
            "en",
            "de"
        ],
        "selectLanguage": true,
        "docMenu": [],
        "monacoToolbox": true
    }
}
